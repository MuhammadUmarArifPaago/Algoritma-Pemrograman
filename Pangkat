x = int(input("Masukkan Angka:"))
y = int(input("Masukkan Pangkat:"))
def pangkat(x,y):
    if y==0:
        return 1
    else:
        return x*pangkat(x,y - 1)
print ("Hasilnya adalah:", pangkat(x,y))
