b = []
z = 0

x = int(input("Masukkan Jumlah:"))

def jumlah(x):
    if x == 0:
        return (x) 
    
for i in range(x):
    y = int(input("Masukkan angka:"))  
    b.append(y)
    z = z + y      
print("jumlahnya adalah", z)
jumlah(x)
