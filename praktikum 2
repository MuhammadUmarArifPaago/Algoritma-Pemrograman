a= int(input("Masukan Nilai a ="))
b= int(input("Masukan Nilai b ="))

import math

tambah = a+b 
selisih = a-b 
hasilkali = a*b 
sisapembagian = a%b 
pembagian = a/b 
pangkat = a**b 

print( "jumlah a ditambah b adalah" ,tambah )
print( "selisih antara a dengan b adalah" ,selisih )
print( "jumlah a dikali b adalah", hasilkali )
print( "jumlah sisa pembagian a dibagi b adalah", sisapembagian )
print( "jumlah a dibagi b adalah",  pembagian )
print( "hasil dari log(a) adalah",  math.log10(a) )
print( "hasil a pangkat b adalah", pangkat )
